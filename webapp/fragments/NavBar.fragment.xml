
<FlexBox xmlns="sap.m" >
    <FlexBox alignItems="Center" justifyContent="Center">
        <IconTabBar 
                id="idNavBar"
                class="sapUiResponsiveContentPadding"
                select=".onSelectTab"
                expandable="false"
                expanded="false">
                <items>
                    <IconTabFilter
                        icon="sap-icon://home"
                        key="home">
                    </IconTabFilter>
                    <IconTabFilter
                        icon="sap-icon://customer"
                        key="profile">
                    </IconTabFilter>
                </items>
            
    </IconTabBar>
            <layoutData>
                <FlexItemData growFactor="4" />
            </layoutData>
    </FlexBox>
</FlexBox>
    